<?xml version="1.0" encoding="UTF-8"?>
<gresources>
  <gresource prefix="/com/rubynetix/connect4">
    <!-- path is relative - "xml-stripblanks" may require you to run '$ sudo apt install libxml2-utils'-->
    <file preprocess="xml-stripblanks">ui/application_window.ui</file>
    <file preprocess="xml-stripblanks">ui/game_window.ui</file>

    <!-- Main application window -->
    <file preprocess="xml-stripblanks">ui/main_window.ui</file>

    <!-- Application 'screens' -->
    <file preprocess="xml-stripblanks">ui/main_menu_widget.ui</file>
    <file preprocess="xml-stripblanks">ui/game_window_widget.ui</file>
  </gresource>
</gresources>